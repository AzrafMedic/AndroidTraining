<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jp.mixi.practice.resourcemanagement" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="jp.mixi.practice.resourcemanagement.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="jp.mixi.practice.resourcemanagement.StringPractice1Activity"
            android:label="@string/string1"
            android:parentActivityName="jp.mixi.practice.resourcemanagement.MainActivity">
        </activity>
        <activity
            android:name="jp.mixi.practice.resourcemanagement.StringPractice2Activity"
            android:label="@string/string2"
            android:parentActivityName="jp.mixi.practice.resourcemanagement.MainActivity">
        </activity>
        <activity
            android:name="jp.mixi.practice.resourcemanagement.DrawablePracticeActivity"
            android:label="@string/drawable"
            android:parentActivityName="jp.mixi.practice.resourcemanagement.MainActivity">
        </activity>
        <activity
            android:name="jp.mixi.practice.resourcemanagement.AnimationPracticeActivity"
            android:label="@string/animation"
            android:parentActivityName="jp.mixi.practice.resourcemanagement.MainActivity">
        </activity>
    </application>

</manifest>
